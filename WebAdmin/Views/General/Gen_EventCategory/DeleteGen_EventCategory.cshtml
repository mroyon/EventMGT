@using CLL
@model BDO.Core.DataAccessObjects.Models.gen_eventcategoryEntity
@inject CLL.Localization.LocalizeService SharedLocalizer

@using Microsoft.Extensions.Options
@{
    ViewData["Title"] = @SharedLocalizer.GetLocalizedHtmlString("DELETE_GEN_EVENTCATEGORY");
}

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-footer">
                <h5 class="card-title">@SharedLocalizer.GetLocalizedHtmlString("DELETE_GEN_EVENTCATEGORY")</h5>
            </div>
            <form method="POST" id="frmDeleteGen_EventCategory" class="my-login-validation" novalidate="" autocomplete="off">
                <div class="card-body">
                    <div class="row">
                        <input asp-for="eventcategoryid" type="hidden" />
                    </div>
                    
					 <div class="row ">
							 <input asp-for="eventcategoryid" type="hidden" />
						 <div class="col-md-6">
							 <div class="form-group">
								 <label class="labelNormal required " asp-for="eventcategory"></label>
								 <textarea cols="40" rows="5" asp-for="eventcategory" class="form-control"></textarea>
								 <span asp-validation-for="eventcategory" class="eventcategoryerror text-danger " id="eventcategoryerror" ></span>
							</div>
						</div>
					</div>
					 <div class="row ">
						 <div class="col-md-6">
							 <div class="form-group">
								 <label class="labelNormal  " asp-for="description"></label>
								 <input asp-for="description" class="form-control" autocomplete="off" readonly="readonly"/> 
								 <span asp-validation-for="description" class="text-danger"></span>
							</div>
						</div>
						 <div class="col-md-6">
							 <div class="form-group">
								 <label class="labelNormal  " asp-for="ex_nvarchar3"></label>
								 <input asp-for="ex_nvarchar3" class="form-control" autocomplete="off" readonly="readonly"/> 
								 <span asp-validation-for="ex_nvarchar3" class="text-danger"></span>
							</div>
						</div>
					</div>
                    
                    
                     <div class="row btn-toolbar">
                        <div class="col-md-6 ">
                            <div class="form-group">
                                <button type="submit" id="btnDeleteGen_EventCategory" data="@Url.Action("LandingGEN_EVENTCATEGORY", "GEN_EVENTCATEGORY")" class="btn btn-danger btn-md btnAddUser"><i class="fa fa-trash-alt"></i> @SharedLocalizer.GetLocalizedHtmlString("DELETE_REQUEST")</button>
                                <button id="btnGoBackGen_EventCategory" data="@Url.Action("LandingGEN_EVENTCATEGORY", "GEN_EVENTCATEGORY")" class="btn btn-secondary btn-md "><i class="fas fa-arrow-circle-left"></i> @SharedLocalizer.GetLocalizedHtmlString("GO_BACK")</button>
                            </div>
                        </div>
                    </div>
                    
                </div>
            </form>
            <!-- ./card-body -->
            <div class="card-footer">
                <div class="row">
                    <div class="col-md-12">

                        <!-- /.description-block -->
                    </div>
                    <!-- /.col -->
                </div>
                <!-- /.row -->
            </div>
            <!-- /.card-footer -->
        </div>
        <!-- /.card -->
    </div>
    <!-- /.col -->
</div>

@section Scripts {
    <script src="~/lib/customjs/Gen_EventCategory/Gen_EventCategory.js"></script>
}





