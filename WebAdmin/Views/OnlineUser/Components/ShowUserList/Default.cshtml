@using CLL
@model BDO.Core.DataAccessObjects.SecurityModels.owin_userEntity
@inject CLL.Localization.LocalizeService SharedLocalizer
@{
    ViewData["Title"] = @SharedLocalizer.GetLocalizedHtmlString("SHOWUSERLIST");
}
<div class="card">
    <div class="card-header">
        @ViewBag.Title
    </div>
    <div class="card-body">
        <form method="POST" id="showonlineuserlistview" class="my-login-validation" novalidate="" autocomplete="off">
            <div class="row">
                <div class="col-md-12">
                    <table id="dtonlineuser" class="table table-striped table-bordered " width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <th><label class="labelNormal  ">User ID</label></th>
                                <th><label class="labelNormal  ">Email Address</label></th>
                                <th>@SharedLocalizer.GetLocalizedHtmlString("ACTIONS_BUTTONS")</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
            <div class="btn-toolbar">
                <div class="form-group">
                    <button type="submit" id="btn-common-modal-close" class="btn btn-primary btn-md cancel"><i class="fa fa-arrow-circle-left"></i> @SharedLocalizer.GetLocalizedHtmlString("CANCEL_REQUEST")</button>
                </div>
            </div>
        </form>
    </div>
    <div class="card-footer">
        <div class="row">
            <div class="col-md-12">

            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </div>
</div>
<script type="text/javascript">

    $(function () {

        if (_onlineUserList != null) {
            ShowDataOnlineUserList();
        }
        $('body').on('click', '#btn-common-modal-close', function (event) {
            try {
                event.preventDefault();
                _cusCloseModal('modal-common');
            } catch (e) {
                showErrorAlert("Error", e.message, "OK");
            }
        });
    });
</script>