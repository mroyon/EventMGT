        .timeline-likes {
            color: #6d767f;
            font-weight: 600;
            font-size: 12px
        }

            .timeline-likes .stats-right {
                float: right
            }

            .timeline-likes .stats-total {
                display: inline-block;
                line-height: 20px
            }

            .timeline-likes .stats-icon {
